<project>
  <name>glic-ci-rating-rules</name>
  <modules>
    <module>
      <rules-root path="rules/*.xlsx"/>
      <method-filter>
        <includes>
          <value>.*DeterminePolicyPremiumDXP.*</value>
          <value>.*DeterminePolicyPremium.*</value>
        </includes>
      </method-filter>
    </module>
  </modules>
  <classpath>
    <entry path="lib/*.jar"/>
  </classpath>
  <properties-file-name-pattern>.*-%effectiveDate:MMddyyyy%-%startRequestDate:MMddyyyy%</properties-file-name-pattern>
  <properties-file-name-processor>org.openl.rules.project.resolving.CWPropertyFileNameProcessor</properties-file-name-processor>
</project>